###############################################
#
# Makefile
#
###############################################

all: cimpl csharpimpl golangimpl javaimpl pythonimpl swiftimpl

cimpl:
	cd c; \
	make

csharpimpl:
	cd csharp; \
	make

golangimpl:
	cd golang; \
	make

javaimpl:
	cd java; \
	make

pythonimpl:
	cd python; \
	make

swiftimpl:
	cd swift; \
	make
